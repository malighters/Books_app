{
  "name": "books_tasks",
  "version": "1.0.0",
  "description": "Books app",
  "main": "lib/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node .",
    "dev": "nodemon --exec ts-node src/index.ts",
    "lint": "eslint --ext .js,.ts .",
    "typeorm": "typeorm-ts-node-esm",
    "typeorm:sync": "npm run typeorm schema:sync -- -d ./src/data-source.ts ",
    "typeorm:drop": "npm run typeorm schema:drop -- -d ./src/data-source.ts ",
    "typeorm:reset": "npm run typeorm:drop && npm run typeorm:sync",
    "typeorm:run": "ts-node -r tsconfig-paths/register $(yarn bin typeorm) migration:run",
    "migration:show": "npm run typeorm migration:show -- -d ./src/data-source.ts ",
    "migration:revert": "npm run typeorm migration:revert -- -d ./src/data-source.ts",
    "migration:run": "npm run typeorm migration:run -- -d ./src/data-source.ts",
    "migration:generate": "npm run typeorm migration:generate -- -d ./src/data-source.ts",
    "migration:create": "npm run typeorm migration:create -- -d ./src/data-source.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "bullmq": "^3.6.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "mongodb": "^3.7.3",
    "mongoose": "^6.10.1",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "pg": "^8.10.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.12"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.6",
    "@types/node": "^18.14.6",
    "@types/passport": "^1.0.12",
    "@types/passport-local": "^1.0.35",
    "eslint": "^8.35.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "nodemon": "^2.0.21",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
